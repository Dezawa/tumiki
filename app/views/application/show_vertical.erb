<h3><%= @TITLE %> </h3>
<% form_for(@model) do |f| %>
<%= hidden_field_tag "page",@page %>
  <%= @model errors.messages  %>
  <%= f.submit '更新' %>
<table border=1><%= label_line_option %>
<% id = @model.id -%>
<tr id='pro_<%= id %>' ><% @labels.each{|html_cell| -%>
  <%= raw html_cell.td -%><%= html_cell.disp(@model) -%></td>
 <%- } -%>
</tr>
</table>
<% end %>
<br />
<hr>
<%= raw @PostMessage %>
